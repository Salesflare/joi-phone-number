  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.1.0",
    "eslint-config-xo-space": "^0.16.0",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "joi": "^11.0.0",
