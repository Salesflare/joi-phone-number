{
  "name": "joi-phone-number",
  "version": "1.0.4",
  "description": "Phone number validation rule for Joi",
  "homepage": "https://github.com/Salesflare/joi-phone-number",
  "author": "Salesflare <developers@salesflare.com> (https://salesflare.com)",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "joi",
    "extention",
    "phone number",
    "rule"
  ],
  "scripts": {
